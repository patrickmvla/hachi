{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@hachi/ui": [
        "../../packages/ui/src"
      ],
      "@hachi/ui/*": [
        "../../packages/ui/src/*"
      ],
      "@hachi/schemas": [
        "../../packages/schemas/src"
      ],
      "@hachi/schemas/*": [
        "../../packages/schemas/src/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
